{
  "name": "youtube_downloader",
  "version": "1.0.0",
  "description": "YouTube Downloader",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "author": "justforgetmeeee",
  "license": "MIT",
  "devDependencies": {
    "electron": "^34.2.0",
    "electron-builder": "^25.1.8"},
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "electron-store": "^10.0.1",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "fs-extra": "^11.3.0",
    "tailwindcss": "^4.0.7",
    "ytdl-core": "npm:@distube/ytdl-core@^4.16.4"
  },
  "build": {
    "appId": "com.justforgetmeeee.youtubedownloader",
    "productName": "YouTube Downloader",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "icon": "icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "icon.ico"
    }
  }
}

